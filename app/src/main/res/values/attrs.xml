<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AVDTransition">
        <attr name="android:src" />
    </declare-styleable>

    <declare-styleable name="ColorTransition">
        <attr name="startColor" format="color" />
        <attr name="endColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CircularReveal">
        <attr name="startRadius" format="dimension" />
        <attr name="endRadius" format="dimension" />
        <attr name="centerOn" format="reference" />
    </declare-styleable>

    <declare-styleable name="RatioImageView">
        <attr name="widthRatio" format="integer" />
        <attr name="heightRatio" format="integer" />
    </declare-styleable>
</resources>